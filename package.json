{
  "name": "webpack-react-ssr-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for using Webpack and SSR React",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && node build/server.js",
    "dev": "npm-run-all -p watch:*",
    "dev:build": "npm-run-all build:dev dev",
    "build": "rm -rf build && webpack --mode=production",
    "build:dev": "rm -rf build && webpack --mode=development",
    "watch:app": "webpack --watch --mode=development",
    "watch:web": "webpack-dev-server --mode=development --inline --hot",
    "watch:server": "nodemon ./build/server.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "html-loader": "^0.5.5",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-node-externals": "^1.7.2"
  }
}
